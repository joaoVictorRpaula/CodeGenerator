@{
    var applicationName = Model.Application.ApplicationName;
}
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="7.0.18" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.18" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\@(applicationName).DOMAIN\@(applicationName).DOMAIN.csproj" />
    <ProjectReference Include="..\@(applicationName).INFRA\@(applicationName).INFRA.csproj" />
  </ItemGroup>

</Project>
